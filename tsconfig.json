{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@interfaces/*": [
        "./common/interfaces/*"
      ],
      "@entities/*": [
        "./common/entities/*"
      ],
      "@common/*": [
        "./common/*"
      ]
    },
    "lib": [
      "ES2021",
      "ES2020",
      "ES2018",
    ]
  }
}
