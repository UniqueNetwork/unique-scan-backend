{
  "name": "polkastats-backend-uniquenetwork",
  "version": "2.0.0",
  "description": "",
  "author": "Unique Network Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod:web-api": "node dist/apps/web-api/main ",
    "lint": "eslint \"{src,apps,libs,test,tests,migrations}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:api:e2e": "jest --config apps/web-api/test/jest-e2e.json apps/web-api",
    "test:e2e": "jest --config apps/web-api/test/jest-e2e.json",
    "test:api:e2e:watch": "jest --watch --config apps/web-api/test/jest-e2e.json apps/web-api",
    "tests": "mocha --timeout 500000 --exit -r ts-node/register './e2e/test/*.test.ts'",
    "tests:start-environment": "docker-compose -f docker-compose-e2e.yml up -d",
    "tests:stop-environment": "docker-compose -f docker-compose-e2e.yml down",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config=common/typeorm.config.ts",
    "db:migration:generate": "npm run typeorm migration:generate -- -n",
    "db:migration:run": "npm run typeorm migration:run",
    "db:migration:create": "npm run typeorm migration:create -- -n"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-express": "^3.6.2",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-node": "^10.4.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@unique-nft/sdk": "^0.3.16",
    "@unique-nft/types": "^0.6.0-4",
    "chai": "^4.3.6",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "json-to-graphql-query": "^2.2.2",
    "mocha": "^9.2.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typeorm-extension": "^2.1.3",
    "typeorm-fixtures-cli": "^1.11.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@interfaces/(.*)$": "<rootDir>/common/interfaces/$1",
      "^@entities/(.*)$": "<rootDir>/common/entities/$1",
      "^@common/(.*)$": "<rootDir>/common/$1"
    }
  }
}
